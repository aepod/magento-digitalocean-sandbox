{
  "name": "puppet-download_file",
  "version": "1.2.1",
  "author": "puppetcommunity",
  "summary": "A module that will download files for use on Windows servers. Requires the server to have PowerShell 2.0 and above",
  "license": "Apache 2.0",
  "source": "https://github.com/puppet-community/puppet-download_file",
  "project_page": "https://github.com/puppet-community/puppet-download_file",
  "issues_url": "https://github.com/puppet-communtiy/puppet-download_file/issues",
  "tags": [
    "windows",
    "utility"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": " windows",
      "operatingsystemrelease": [
        "2008R2",
        "2012",
        "2012R2"
      ]
    }
  ],
  "dependencies": [
    {"name":"puppetlabs/stdlib","version_requirement":">=3.0.0 <5.0.0"},
    {"name":"puppetlabs/powershell","version_requirement":">= 1.0.1"}
  ]
}
